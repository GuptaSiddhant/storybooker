{
  "name": "@storybooker/gcp",
  "version": "0.25.0",
  "type": "module",
  "description": "GCP adapters for Storybooker",
  "author": {
    "name": "Siddhant Gupta",
    "url": "https://guptasiddhant.com"
  },
  "homepage": "https://storybooker.js.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/GuptaSiddhant/storybooker",
    "directory": "packages/gcp"
  },
  "files": [
    "src",
    "dist"
  ],
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "node ../../scripts/build-adapter.ts",
    "dev": "node ../../scripts/build-adapter.ts -w",
    "fmt": "oxfmt -c ../../.oxfmtrc.json ./src",
    "lint": "oxlint -c ../../.oxlintrc.json --type-aware --type-check --import-plugin ./src",
    "lint:fix": "yarn lint --fix"
  },
  "tsdown-entry": {
    "big-table": "./src/gcp-big-table.ts",
    "firestore": "./src/gcp-firestore.ts",
    "storage": "./src/gcp-storage.ts"
  },
  "dependencies": {
    "storybooker": "workspace:*"
  },
  "devDependencies": {
    "@google-cloud/bigtable": "^6.0.0",
    "@google-cloud/firestore": "^7.0.0",
    "@google-cloud/storage": "^7.0.0",
    "oxfmt": "catalog:",
    "oxlint": "catalog:",
    "oxlint-tsgolint": "catalog:",
    "tsdown": "catalog:"
  },
  "peerDependencies": {
    "@google-cloud/bigtable": "^6.0.0",
    "@google-cloud/firestore": "^7.0.0",
    "@google-cloud/storage": "^7.0.0"
  },
  "peerDependenciesMeta": {
    "@google-cloud/bigtable": {
      "optional": true
    },
    "@google-cloud/firestore": {
      "optional": true
    },
    "@google-cloud/storage": {
      "optional": true
    }
  },
  "exports": {
    "./big-table": "./dist/big-table.mjs",
    "./firestore": "./dist/firestore.mjs",
    "./storage": "./dist/storage.mjs",
    "./package.json": "./package.json"
  }
}
