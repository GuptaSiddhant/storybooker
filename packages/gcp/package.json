{
  "name": "@storybooker/gcp",
  "version": "0.20.1",
  "type": "module",
  "description": "StoryBooker Adapter for interacting with GCP services.",
  "author": {
    "name": "Siddhant Gupta",
    "url": "https://guptasiddhant.com"
  },
  "homepage": "https://storybooker.js.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/guptasiddhant/storybooker",
    "directory": "packages/gcp"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guptasiddhant/storybooker/issues"
  },
  "files": [
    "src",
    "dist"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "sideEffects": false,
  "keywords": [
    "gcp",
    "google-cloud",
    "google",
    "functions",
    "gcp-functions",
    "storybook",
    "storybooks",
    "storybooker",
    "chromatic",
    "storage",
    "blob",
    "upload",
    "table",
    "compress",
    "builds",
    "labels",
    "projects",
    "cosmos"
  ],
  "exports": {
    "./big-table": {
      "source": "./src/big-table.ts",
      "default": "./dist/big-table.mjs"
    },
    "./firestore": {
      "source": "./src/firestore.ts",
      "default": "./dist/firestore.mjs"
    },
    "./storage": {
      "source": "./src/storage.ts",
      "default": "./dist/storage.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown -w ./src",
    "fmt": "oxfmt -c ../../.oxfmtrc.json ./src",
    "lint": "oxlint --type-aware --type-check ./src",
    "lint:fix": "yarn lint --fix"
  },
  "dependencies": {
    "@storybooker/core": "workspace:*"
  },
  "devDependencies": {
    "@google-cloud/bigtable": "^6.4.1",
    "@google-cloud/firestore": "^7.11.5",
    "@google-cloud/storage": "^7.17.1",
    "@types/node": "catalog:",
    "@typescript/native-preview": "catalog:",
    "oxfmt": "catalog:",
    "oxlint": "catalog:",
    "oxlint-tsgolint": "catalog:",
    "tsdown": "catalog:"
  },
  "peerDependencies": {
    "@google-cloud/bigtable": "^6.0.0",
    "@google-cloud/firestore": "^7.0.0",
    "@google-cloud/storage": "^7.0.0"
  },
  "peerDependenciesMeta": {
    "@google-cloud/bigtable": {
      "optional": true
    },
    "@google-cloud/firestore": {
      "optional": true
    },
    "@google-cloud/storage": {
      "optional": true
    }
  },
  "main": "./dist/storage.cjs",
  "module": "./dist/storage.js",
  "types": "./dist/storage.d.cts",
  "publishConfig": {
    "exports": {
      "./big-table": "./dist/big-table.mjs",
      "./firestore": "./dist/firestore.mjs",
      "./storage": "./dist/storage.mjs",
      "./package.json": "./package.json"
    }
  }
}
