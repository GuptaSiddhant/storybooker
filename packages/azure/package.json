{
  "name": "@storybooker/azure",
  "version": "0.12.0",
  "type": "module",
  "description": "StoryBooker Adapter for interacting with Azure services.",
  "author": {
    "name": "Siddhant Gupta",
    "url": "https://guptasiddhant.com"
  },
  "homepage": "https://storybooker.js.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/guptasiddhant/storybooker",
    "directory": "packages/azure"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guptasiddhant/storybooker/issues"
  },
  "files": [
    "src",
    "dist"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "sideEffects": false,
  "keywords": [
    "azure",
    "functions",
    "azure-functions",
    "storybook",
    "storybooks",
    "storybooker",
    "chromatic",
    "storage",
    "blob",
    "upload",
    "table",
    "compress",
    "builds",
    "labels",
    "projects",
    "cosmos"
  ],
  "exports": {
    "./blob-storage": {
      "source": "./src/blob-storage.ts",
      "default": "./dist/blob-storage.js"
    },
    "./cosmos-db": {
      "source": "./src/cosmos-db.ts",
      "default": "./dist/cosmos-db.js"
    },
    "./data-tables": {
      "source": "./src/data-tables.ts",
      "default": "./dist/data-tables.js"
    },
    "./easy-auth": {
      "source": "./src/easy-auth.ts",
      "default": "./dist/easy-auth.js"
    },
    "./functions": {
      "source": "./src/functions.ts",
      "default": "./dist/functions.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "check": "tsgo --noEmit",
    "dev": "tsdown -w ./src",
    "fmt": "prettier src --write --config ../../.prettierrc",
    "lint": "oxlint --type-aware ./src",
    "lint:fix": "yarn lint --fix",
    "publish:jsr": "npx -y jsr publish",
    "start": "func start"
  },
  "dependencies": {
    "@storybooker/core": "workspace:^"
  },
  "devDependencies": {
    "@azure/cosmos": "^4.5.1",
    "@azure/data-tables": "^13.0.0",
    "@azure/functions": "^4.0.0",
    "@azure/storage-blob": "^12.0.0",
    "@types/node": "^22.0.0",
    "@typescript/native-preview": "^7.0.0-dev.20251024.1",
    "oxlint": "^1.24.0",
    "oxlint-tsgolint": "^0.3.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.9"
  },
  "peerDependencies": {
    "@azure/cosmos": "^4.5.1",
    "@azure/data-tables": "^13.0.0",
    "@azure/functions": "^4.0.0",
    "@azure/storage-blob": "^12.0.0"
  },
  "peerDependenciesMeta": {
    "@azure/cosmos": {
      "optional": true
    },
    "@azure/data-tables": {
      "optional": true
    },
    "@azure/functions": {
      "optional": true
    },
    "@azure/storage-blob": {
      "optional": true
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "publishConfig": {
    "exports": {
      "./blob-storage": "./dist/blob-storage.js",
      "./cosmos-db": "./dist/cosmos-db.js",
      "./data-tables": "./dist/data-tables.js",
      "./easy-auth": "./dist/easy-auth.js",
      "./functions": "./dist/functions.js",
      "./package.json": "./package.json"
    }
  }
}
