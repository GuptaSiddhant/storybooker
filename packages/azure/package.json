{
  "name": "@storybooker/azure",
  "version": "0.20.0",
  "type": "module",
  "description": "StoryBooker Adapter for interacting with Azure services.",
  "author": {
    "name": "Siddhant Gupta",
    "url": "https://guptasiddhant.com"
  },
  "homepage": "https://storybooker.js.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/guptasiddhant/storybooker",
    "directory": "packages/azure"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guptasiddhant/storybooker/issues"
  },
  "files": [
    "src",
    "dist"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "sideEffects": false,
  "keywords": [
    "azure",
    "functions",
    "azure-functions",
    "storybook",
    "storybooks",
    "storybooker",
    "chromatic",
    "storage",
    "blob",
    "upload",
    "table",
    "compress",
    "builds",
    "labels",
    "projects",
    "cosmos"
  ],
  "exports": {
    "./blob-storage": {
      "source": "./src/blob-storage.ts",
      "default": "./dist/blob-storage.mjs"
    },
    "./cosmos-db": {
      "source": "./src/cosmos-db.ts",
      "default": "./dist/cosmos-db.mjs"
    },
    "./data-tables": {
      "source": "./src/data-tables.ts",
      "default": "./dist/data-tables.mjs"
    },
    "./easy-auth": {
      "source": "./src/easy-auth.ts",
      "default": "./dist/easy-auth.mjs"
    },
    "./functions": {
      "source": "./src/functions.ts",
      "default": "./dist/functions.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown -w ./src",
    "fmt": "prettier src --write --config ../../.prettierrc",
    "oxfmt": "oxfmt ./src",
    "lint": "oxlint --type-aware --type-check ./src",
    "lint:fix": "yarn lint --fix",
    "start": "func start"
  },
  "dependencies": {
    "@storybooker/core": "workspace:*",
    "@storybooker/ui": "workspace:*"
  },
  "devDependencies": {
    "@azure/cosmos": "^4.5.1",
    "@azure/data-tables": "^13.0.0",
    "@azure/functions": "^4.0.0",
    "@azure/storage-blob": "^12.0.0",
    "@types/node": "catalog:",
    "@typescript/native-preview": "catalog:",
    "oxfmt": "catalog:",
    "oxlint": "catalog:",
    "oxlint-tsgolint": "catalog:",
    "prettier": "catalog:",
    "tsdown": "catalog:"
  },
  "peerDependencies": {
    "@azure/cosmos": "^4.5.1",
    "@azure/data-tables": "^13.0.0",
    "@azure/functions": "^4.0.0",
    "@azure/storage-blob": "^12.0.0"
  },
  "peerDependenciesMeta": {
    "@azure/cosmos": {
      "optional": true
    },
    "@azure/data-tables": {
      "optional": true
    },
    "@azure/functions": {
      "optional": true
    },
    "@azure/storage-blob": {
      "optional": true
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "publishConfig": {
    "exports": {
      "./blob-storage": "./dist/blob-storage.mjs",
      "./cosmos-db": "./dist/cosmos-db.mjs",
      "./data-tables": "./dist/data-tables.mjs",
      "./easy-auth": "./dist/easy-auth.mjs",
      "./functions": "./dist/functions.mjs",
      "./package.json": "./package.json"
    }
  }
}
