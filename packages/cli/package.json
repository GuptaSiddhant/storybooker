{
  "name": "storybooker",
  "version": "0.20.0",
  "type": "module",
  "bin": "./dist/index.js",
  "description": "Storybooker CLI for uploading builds and files.",
  "author": {
    "name": "Siddhant Gupta",
    "url": "https://guptasiddhant.com"
  },
  "files": [
    "src",
    "dist"
  ],
  "homepage": "https://storybooker.js.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/guptasiddhant/storybooker",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "cli": "node ./dist/index.js --cwd ../../apps/test-ui",
    "dev": "tsdown -w ./src",
    "fmt": "prettier src --write --config ../../.prettierrc",
    "oxfmt": "oxfmt ./src",
    "lint": "oxlint --type-aware --type-check ./src",
    "lint:fix": "yarn lint --fix",
    "openapi": "openapi-typescript ../core/dist/openapi.json -o ./src/service-schema.d.ts",
    "start": "node --watch ./dist/index.js --cwd ../../apps/test-ui"
  },
  "dependencies": {
    "openapi-fetch": "^0.14.0",
    "yargs": "^18.0.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@types/yargs": "^17.0.33",
    "@typescript/native-preview": "catalog:",
    "openapi-typescript": "^7.9.1",
    "oxfmt": "catalog:",
    "oxlint": "catalog:",
    "oxlint-tsgolint": "catalog:",
    "prettier": "catalog:",
    "tsdown": "catalog:",
    "typescript": "^5.9.2"
  }
}
