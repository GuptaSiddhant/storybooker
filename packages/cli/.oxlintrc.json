{
  "extends": ["../../.oxlintrc.json"],
  "env": {
    "builtin": true,
    "node": true
  },
  "rules": {
    "no-console": "allow",
    "no-process-exit": "allow",
    "no-unsafe-call": "allow",
    "sort-keys": "allow"
  },
  "ignorePatterns": [],
  "overrides": [
    {
      "files": ["*.d.ts"],
      "rules": {
        "max-lines": "allow",
        "consistent-indexed-object-style": "allow",
        "no-redundant-type-constituents": "allow"
      }
    }
  ]
}
