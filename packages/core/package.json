{
  "name": "@storybooker/core",
  "version": "0.16.0",
  "type": "module",
  "description": "Storybooker platform agnostic router core.",
  "author": {
    "name": "Siddhant Gupta",
    "url": "https://guptasiddhant.com"
  },
  "homepage": "https://storybooker.js.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/guptasiddhant/storybooker",
    "directory": "packages/core"
  },
  "files": [
    "src",
    "dist"
  ],
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "check": "tsgo --noEmit",
    "dev": "tsdown -w ./src",
    "fmt": "prettier src --write --config ../../.prettierrc",
    "lint": "oxlint --type-aware ./src",
    "lint:fix": "yarn lint --fix",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "^1.1.5",
    "@remix-run/headers": "^0.16.0",
    "decompress": "^4.0.0",
    "hono": "^4.10.6",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@types/decompress": "^4.0.0",
    "@types/node": "^22.0.0",
    "@typescript/native-preview": "^7.0.0-dev.20251024.1",
    "@vitest/coverage-v8": "^3.2.4",
    "glob": "^10.0.0",
    "js-yaml": "^4.1.1",
    "oxlint": "^1.24.0",
    "oxlint-tsgolint": "^0.3.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.9",
    "vitest": "^3.2.4"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./adapter": {
      "source": "./src/adapters/index.ts",
      "default": "./dist/adapter.js"
    },
    "./constants": {
      "source": "./src/utils/constants.ts",
      "default": "./dist/constants.js"
    },
    "./mimes": {
      "source": "./src/utils/mime-utils.ts",
      "default": "./dist/mimes.js"
    },
    "./router": {
      "source": "./src/routers/_app-router.ts",
      "default": "./dist/router.js"
    },
    "./types": {
      "source": "./src/types.ts",
      "default": "./dist/types.js"
    },
    "./url": {
      "source": "./src/urls.ts",
      "default": "./dist/url.js"
    },
    "./utils": {
      "source": "./src/utils/index.ts",
      "default": "./dist/utils.js"
    },
    "./package.json": "./package.json",
    "./openapi.json": "./dist/openapi.json",
    "./openapi.yaml": "./dist/openapi.yaml"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.js",
      "./adapter": "./dist/adapter.js",
      "./constants": "./dist/constants.js",
      "./mimes": "./dist/mimes.js",
      "./router": "./dist/router.js",
      "./types": "./dist/types.js",
      "./url": "./dist/url.js",
      "./utils": "./dist/utils.js",
      "./package.json": "./package.json"
    }
  }
}
