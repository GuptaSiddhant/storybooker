{
  "name": "storybooker",
  "version": "0.21.0",
  "type": "module",
  "description": "Manage Storybook instances on your own infrastructure with ease.",
  "author": {
    "name": "Siddhant Gupta",
    "url": "https://guptasiddhant.com"
  },
  "homepage": "https://storybooker.js.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/GuptaSiddhant/storybooker",
    "directory": "packages/core"
  },
  "files": [
    "adapters",
    "src",
    "dist",
    "openapi.json"
  ],
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown -w",
    "fmt": "oxfmt -c ../../.oxfmtrc.json ./src",
    "lint": "oxlint --type-aware --type-check --import-plugin ./src",
    "lint:fix": "yarn lint --fix",
    "openapi": "node ./scripts/gen-openapi-json.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "^1.1.5",
    "@remix-run/headers": "^0.16.0",
    "decompress": "^4.0.0",
    "hono": "^4.11.0",
    "js-yaml": "^4.1.1",
    "zod": "^4.2.0"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@azure/cosmos": "^4.5.1",
    "@azure/data-tables": "^13.0.0",
    "@azure/functions": "^4.0.0",
    "@azure/storage-blob": "^12.0.0",
    "@google-cloud/bigtable": "^6.0.0",
    "@google-cloud/firestore": "^7.0.0",
    "@google-cloud/storage": "^7.0.0",
    "@types/decompress": "^4.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "catalog:",
    "@typescript/native-preview": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "oxfmt": "catalog:",
    "oxlint": "catalog:",
    "oxlint-tsgolint": "catalog:",
    "redis": "^5.10.0",
    "tsdown": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@azure/cosmos": "^4.0.0",
    "@azure/data-tables": "^13.0.0",
    "@azure/functions": "^4.0.0",
    "@azure/storage-blob": "^12.0.0",
    "@google-cloud/bigtable": "^6.0.0",
    "@google-cloud/firestore": "^7.0.0",
    "@google-cloud/storage": "^7.0.0",
    "redis": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-dynamodb": {
      "optional": true
    },
    "@aws-sdk/client-s3": {
      "optional": true
    },
    "@azure/cosmos": {
      "optional": true
    },
    "@azure/data-tables": {
      "optional": true
    },
    "@azure/functions": {
      "optional": true
    },
    "@azure/storage-blob": {
      "optional": true
    },
    "@google-cloud/bigtable": {
      "optional": true
    },
    "@google-cloud/firestore": {
      "optional": true
    },
    "@google-cloud/storage": {
      "optional": true
    },
    "redis": {
      "optional": true
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "default": "./dist/index.mjs"
    },
    "./adapter": {
      "source": "./src/adapters/_internal/index.ts",
      "default": "./dist/adapter.mjs"
    },
    "./adapter/~auth": {
      "source": "./src/adapters/_internal/auth.ts",
      "default": "./dist/adapter/~auth.mjs"
    },
    "./adapter/~database": {
      "source": "./src/adapters/_internal/database.ts",
      "default": "./dist/adapter/~database.mjs"
    },
    "./adapter/~logger": {
      "source": "./src/adapters/_internal/logger.ts",
      "default": "./dist/adapter/~logger.mjs"
    },
    "./adapter/~storage": {
      "source": "./src/adapters/_internal/storage.ts",
      "default": "./dist/adapter/~storage.mjs"
    },
    "./adapter/~ui": {
      "source": "./src/adapters/_internal/ui.ts",
      "default": "./dist/adapter/~ui.mjs"
    },
    "./adapter/aws-dynamodb": {
      "source": "./src/adapters/aws-dynamodb.ts",
      "default": "./dist/adapter/aws-dynamodb.mjs"
    },
    "./adapter/aws-s3": {
      "source": "./src/adapters/aws-s3.ts",
      "default": "./dist/adapter/aws-s3.mjs"
    },
    "./adapter/azure-blob-storage": {
      "source": "./src/adapters/azure-blob-storage.ts",
      "default": "./dist/adapter/azure-blob-storage.mjs"
    },
    "./adapter/azure-cosmos-db": {
      "source": "./src/adapters/azure-cosmos-db.ts",
      "default": "./dist/adapter/azure-cosmos-db.mjs"
    },
    "./adapter/azure-data-tables": {
      "source": "./src/adapters/azure-data-tables.ts",
      "default": "./dist/adapter/azure-data-tables.mjs"
    },
    "./adapter/azure-easy-auth": {
      "source": "./src/adapters/azure-easy-auth.ts",
      "default": "./dist/adapter/azure-easy-auth.mjs"
    },
    "./adapter/azure-functions": {
      "source": "./src/adapters/azure-functions.ts",
      "default": "./dist/adapter/azure-functions.mjs"
    },
    "./adapter/fs": {
      "source": "./src/adapters/fs.ts",
      "default": "./dist/adapter/fs.mjs"
    },
    "./adapter/gcp-big-table": {
      "source": "./src/adapters/gcp-big-table.ts",
      "default": "./dist/adapter/gcp-big-table.mjs"
    },
    "./adapter/gcp-firestore": {
      "source": "./src/adapters/gcp-firestore.ts",
      "default": "./dist/adapter/gcp-firestore.mjs"
    },
    "./adapter/gcp-storage": {
      "source": "./src/adapters/gcp-storage.ts",
      "default": "./dist/adapter/gcp-storage.mjs"
    },
    "./adapter/mysql": {
      "source": "./src/adapters/mysql.ts",
      "default": "./dist/adapter/mysql.mjs"
    },
    "./adapter/redis": {
      "source": "./src/adapters/redis.ts",
      "default": "./dist/adapter/redis.mjs"
    },
    "./constants": {
      "source": "./src/utils/constants.ts",
      "default": "./dist/constants.mjs"
    },
    "./mimes": {
      "source": "./src/utils/mime-utils.ts",
      "default": "./dist/mimes.mjs"
    },
    "./router": {
      "source": "./src/routers/_app-router.ts",
      "default": "./dist/router.mjs"
    },
    "./types": {
      "source": "./src/types.ts",
      "default": "./dist/types.mjs"
    },
    "./url": {
      "source": "./src/urls.ts",
      "default": "./dist/url.mjs"
    },
    "./utils": {
      "source": "./src/utils/index.ts",
      "default": "./dist/utils.mjs"
    },
    "./package.json": "./package.json",
    "./openapi.json": "./openapi.json"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.mjs",
      "./adapter": "./dist/adapter.mjs",
      "./adapter/~auth": "./dist/adapter/~auth.mjs",
      "./adapter/~database": "./dist/adapter/~database.mjs",
      "./adapter/~logger": "./dist/adapter/~logger.mjs",
      "./adapter/~storage": "./dist/adapter/~storage.mjs",
      "./adapter/~ui": "./dist/adapter/~ui.mjs",
      "./adapter/aws-dynamodb": "./dist/adapter/aws-dynamodb.mjs",
      "./adapter/aws-s3": "./dist/adapter/aws-s3.mjs",
      "./adapter/azure-blob-storage": "./dist/adapter/azure-blob-storage.mjs",
      "./adapter/azure-cosmos-db": "./dist/adapter/azure-cosmos-db.mjs",
      "./adapter/azure-data-tables": "./dist/adapter/azure-data-tables.mjs",
      "./adapter/azure-easy-auth": "./dist/adapter/azure-easy-auth.mjs",
      "./adapter/azure-functions": "./dist/adapter/azure-functions.mjs",
      "./adapter/fs": "./dist/adapter/fs.mjs",
      "./adapter/gcp-big-table": "./dist/adapter/gcp-big-table.mjs",
      "./adapter/gcp-firestore": "./dist/adapter/gcp-firestore.mjs",
      "./adapter/gcp-storage": "./dist/adapter/gcp-storage.mjs",
      "./adapter/mysql": "./dist/adapter/mysql.mjs",
      "./adapter/redis": "./dist/adapter/redis.mjs",
      "./constants": "./dist/constants.mjs",
      "./mimes": "./dist/mimes.mjs",
      "./router": "./dist/router.mjs",
      "./types": "./dist/types.mjs",
      "./url": "./dist/url.mjs",
      "./utils": "./dist/utils.mjs",
      "./package.json": "./package.json"
    }
  }
}
