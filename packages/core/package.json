{
  "name": "@storybooker/core",
  "version": "0.6.0",
  "type": "module",
  "description": "Storybooker platform agnostic router core.",
  "author": {
    "name": "Siddhant Gupta",
    "url": "https://guptasiddhant.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guptasiddhant/storybooker",
    "directory": "packages/core"
  },
  "files": [
    "src",
    "dist",
    "CHANGELOG.md"
  ],
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "check": "tsgo --noEmit",
    "dev": "tsdown -w ./src",
    "fmt": "prettier src --write --config ../../.prettierrc",
    "lint": "oxlint --type-aware ./src",
    "lint:fix": "yarn lint --fix",
    "xss": "xss-scan"
  },
  "imports": {
    "#components/*": "./src/components/*.tsx",
    "#builds/*": "./src/builds/*.ts",
    "#builds-ui/*": "./src/builds/ui/*.tsx",
    "#constants": "./src/utils/constants.ts",
    "#labels/*": "./src/labels/*.ts",
    "#labels-ui/*": "./src/labels/ui/*.tsx",
    "#projects/*": "./src/projects/*.ts",
    "#projects-ui/*": "./src/projects/ui/*.tsx",
    "#router": "./src/router.ts",
    "#store": "./src/utils/store.ts",
    "#types": "./src/types.d.ts",
    "#urls": "./src/urls.ts",
    "#utils/*": "./src/utils/*.ts"
  },
  "dependencies": {
    "@kitajs/html": "^4.0.0",
    "@remix-run/route-pattern": "^0.10.0",
    "decompress": "^4.0.0",
    "zod": "^4.0.0",
    "zod-openapi": "^5.0.0"
  },
  "devDependencies": {
    "@kitajs/ts-html-plugin": "^4.0.0",
    "@types/decompress": "^4.0.0",
    "@types/node": "^22.0.0",
    "@typescript/native-preview": "^7.0.0-dev.20250823.8",
    "glob": "^10.0.0",
    "oxlint": "^1.12.0",
    "oxlint-tsgolint": "^0.0.4",
    "prettier": "^3.6.2",
    "tsdown": "^0.14.1"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./constants": {
      "source": "./src/utils/constants.ts",
      "import": "./dist/constants.js",
      "require": "./dist/constants.cjs"
    },
    "./router": {
      "source": "./src/router.ts",
      "import": "./dist/router.js",
      "require": "./dist/router.cjs"
    },
    "./translations": {
      "source": "./src/translations/index.ts",
      "import": "./dist/translations.js",
      "require": "./dist/translations.cjs"
    },
    "./translations/en-gb": {
      "source": "./src/translations/en-gb.ts",
      "import": "./dist/translations/en-gb.js",
      "require": "./dist/translations/en-gb.cjs"
    },
    "./types": {
      "source": "./src/types.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./utils": {
      "source": "./src/utils/index.ts",
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    },
    "./package.json": "./package.json",
    "./openapi.json": "./dist/openapi.json"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "./constants": {
        "import": "./dist/constants.js",
        "require": "./dist/constants.cjs"
      },
      "./router": {
        "import": "./dist/router.js",
        "require": "./dist/router.cjs"
      },
      "./translations": {
        "import": "./dist/translations.js",
        "require": "./dist/translations.cjs"
      },
      "./translations/en-gb": {
        "import": "./dist/translations/en-gb.js",
        "require": "./dist/translations/en-gb.cjs"
      },
      "./types": {
        "import": "./dist/types.js",
        "require": "./dist/types.cjs"
      },
      "./utils": {
        "import": "./dist/utils.js",
        "require": "./dist/utils.cjs"
      },
      "./package.json": "./package.json"
    }
  }
}
