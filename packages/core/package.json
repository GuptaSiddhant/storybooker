{
  "name": "storybooker",
  "version": "0.23.1",
  "type": "module",
  "description": "Manage Storybook instances on your own infrastructure with ease.",
  "author": {
    "name": "Siddhant Gupta",
    "url": "https://guptasiddhant.com"
  },
  "homepage": "https://storybooker.js.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/GuptaSiddhant/storybooker",
    "directory": "packages/core"
  },
  "files": [
    "adapters",
    "src",
    "dist",
    "openapi.json"
  ],
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown -w",
    "fmt": "oxfmt -c ../../.oxfmtrc.json ./src",
    "lint": "oxlint --type-aware --type-check --import-plugin ./src",
    "lint:fix": "yarn lint --fix",
    "openapi": "node ./scripts/gen-openapi-json.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "^1.1.5",
    "@remix-run/headers": "^0.16.0",
    "decompress": "^4.0.0",
    "hono": "^4.11.0",
    "js-yaml": "^4.1.1",
    "zod": "^4.2.0"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@azure/cosmos": "^4.5.1",
    "@azure/data-tables": "^13.0.0",
    "@azure/functions": "^4.0.0",
    "@azure/storage-blob": "^12.0.0",
    "@google-cloud/bigtable": "^6.0.0",
    "@google-cloud/firestore": "^7.0.0",
    "@google-cloud/storage": "^7.0.0",
    "@types/decompress": "^4.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "catalog:",
    "@typescript/native-preview": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "oxfmt": "catalog:",
    "oxlint": "catalog:",
    "oxlint-tsgolint": "catalog:",
    "redis": "^5.10.0",
    "tsdown": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@azure/cosmos": "^4.0.0",
    "@azure/data-tables": "^13.0.0",
    "@azure/functions": "^4.0.0",
    "@azure/storage-blob": "^12.0.0",
    "@google-cloud/bigtable": "^6.0.0",
    "@google-cloud/firestore": "^7.0.0",
    "@google-cloud/storage": "^7.0.0",
    "redis": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-dynamodb": {
      "optional": true
    },
    "@aws-sdk/client-s3": {
      "optional": true
    },
    "@azure/cosmos": {
      "optional": true
    },
    "@azure/data-tables": {
      "optional": true
    },
    "@azure/functions": {
      "optional": true
    },
    "@azure/storage-blob": {
      "optional": true
    },
    "@google-cloud/bigtable": {
      "optional": true
    },
    "@google-cloud/firestore": {
      "optional": true
    },
    "@google-cloud/storage": {
      "optional": true
    },
    "redis": {
      "optional": true
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./dist/index.mjs",
    "./_internal/adapter": "./dist/_internal/adapter.mjs",
    "./_internal/adapter/auth": "./dist/_internal/adapter/auth.mjs",
    "./_internal/adapter/database": "./dist/_internal/adapter/database.mjs",
    "./_internal/adapter/logger": "./dist/_internal/adapter/logger.mjs",
    "./_internal/adapter/storage": "./dist/_internal/adapter/storage.mjs",
    "./_internal/adapter/ui": "./dist/_internal/adapter/ui.mjs",
    "./_internal/constants": "./dist/_internal/constants.mjs",
    "./_internal/router": "./dist/_internal/router.mjs",
    "./_internal/types": "./dist/_internal/types.mjs",
    "./_internal/utils": "./dist/_internal/utils.mjs",
    "./aws-dynamodb": "./dist/aws-dynamodb.mjs",
    "./aws-s3": "./dist/aws-s3.mjs",
    "./azure-blob-storage": "./dist/azure-blob-storage.mjs",
    "./azure-cosmos-db": "./dist/azure-cosmos-db.mjs",
    "./azure-data-tables": "./dist/azure-data-tables.mjs",
    "./azure-easy-auth": "./dist/azure-easy-auth.mjs",
    "./azure-functions": "./dist/azure-functions.mjs",
    "./fs": "./dist/fs.mjs",
    "./gcp-big-table": "./dist/gcp-big-table.mjs",
    "./gcp-firestore": "./dist/gcp-firestore.mjs",
    "./gcp-storage": "./dist/gcp-storage.mjs",
    "./mysql": "./dist/mysql.mjs",
    "./redis": "./dist/redis.mjs",
    "./package.json": "./package.json"
  }
