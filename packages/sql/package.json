{
  "name": "@storybooker/sql",
  "version": "0.19.4",
  "type": "module",
  "description": "StoryBooker Adapter for interacting with SQL services.",
  "author": {
    "name": "Siddhant Gupta",
    "url": "https://guptasiddhant.com"
  },
  "homepage": "https://storybooker.js.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/guptasiddhant/storybooker",
    "directory": "packages/sql"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guptasiddhant/storybooker/issues"
  },
  "files": [
    "src",
    "dist"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "sideEffects": false,
  "keywords": [
    "functions",
    "storybook",
    "storybooks",
    "storybooker",
    "chromatic",
    "storage",
    "blob",
    "upload",
    "table",
    "compress",
    "builds",
    "labels",
    "projects",
    "sql",
    "mysql",
    "sqlite",
    "postgres",
    "database"
  ],
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "default": "./dist/index.mjs"
    },
    "./mysql": {
      "source": "./src/mysql.ts",
      "default": "./dist/mysql.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown -w ./src",
    "fmt": "prettier src --write --config ../../.prettierrc",
    "oxfmt": "npx oxfmt ./src",
    "lint": "oxlint --type-aware --type-check ./src",
    "lint:fix": "yarn lint --fix"
  },
  "dependencies": {
    "@storybooker/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@typescript/native-preview": "catalog:",
    "oxlint": "catalog:",
    "oxlint-tsgolint": "catalog:",
    "prettier": "catalog:",
    "tsdown": "catalog:"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "publishConfig": {
    "exports": {
      ".": "./dist/index.mjs",
      "./mysql": "./dist/mysql.mjs",
      "./package.json": "./package.json"
    }
  }
}
