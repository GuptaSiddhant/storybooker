{
  "name": "@storybooker/aws",
  "version": "0.19.4",
  "type": "module",
  "description": "StoryBooker Adapter for interacting with AWS services.",
  "author": {
    "name": "Siddhant Gupta",
    "url": "https://guptasiddhant.com"
  },
  "homepage": "https://storybooker.js.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/guptasiddhant/storybooker",
    "directory": "packages/aws"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guptasiddhant/storybooker/issues"
  },
  "files": [
    "src",
    "dist"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "keywords": [
    "amazon",
    "amazon-web-services",
    "aws",
    "dynamodb",
    "storybook",
    "storybooks",
    "storybooker",
    "chromatic",
    "storage",
    "blob",
    "upload",
    "compress",
    "builds",
    "labels",
    "projects",
    "lambda"
  ],
  "sideEffects": false,
  "exports": {
    "./dynamo-db": {
      "source": "./src/dynamo-db.ts",
      "default": "./dist/dynamo-db.mjs"
    },
    "./s3": {
      "source": "./src/s3.ts",
      "default": "./dist/s3.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown -w ./src",
    "fmt": "prettier src --write --config ../../.prettierrc",
    "oxfmt": "npx oxfmt ./src",
    "lint": "oxlint --type-aware --type-check ./src",
    "lint:fix": "yarn lint --fix"
  },
  "dependencies": {
    "@storybooker/core": "workspace:*"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.895.0",
    "@aws-sdk/client-s3": "^3.895.0",
    "@types/node": "catalog:",
    "@typescript/native-preview": "catalog:",
    "oxlint": "catalog:",
    "oxlint-tsgolint": "catalog:",
    "prettier": "catalog:",
    "tsdown": "catalog:"
  },
  "peerDependencies": {
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-dynamodb": {
      "optional": true
    },
    "@aws-sdk/client-s3": {
      "optional": true
    }
  },
  "publishConfig": {
    "exports": {
      "./dynamo-db": "./dist/dynamo-db.mjs",
      "./s3": "./dist/s3.mjs",
      "./package.json": "./package.json"
    }
  }
}
