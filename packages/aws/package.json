{
  "name": "@storybooker/aws",
  "version": "0.12.0",
  "type": "module",
  "description": "StoryBooker Adapter for interacting with AWS services.",
  "author": {
    "name": "Siddhant Gupta",
    "url": "https://guptasiddhant.com"
  },
  "homepage": "https://storybooker.js.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/guptasiddhant/storybooker",
    "directory": "packages/aws"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guptasiddhant/storybooker/issues"
  },
  "files": [
    "src",
    "dist"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "keywords": [
    "amazon",
    "amazon-web-services",
    "aws",
    "dynamodb",
    "storybook",
    "storybooks",
    "storybooker",
    "chromatic",
    "storage",
    "blob",
    "upload",
    "compress",
    "builds",
    "labels",
    "projects"
  ],
  "sideEffects": false,
  "exports": {
    "./dynamo-db": {
      "source": "./src/dynamo-db.ts",
      "default": "./dist/dynamo-db.js"
    },
    "./lambda": {
      "source": "./src/lambda.ts",
      "default": "./dist/lambda.js"
    },
    "./s3": {
      "source": "./src/s3.ts",
      "default": "./dist/s3.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "check": "tsgo --noEmit",
    "dev": "tsdown -w ./src",
    "fmt": "prettier src --write --config ../../.prettierrc",
    "lint": "oxlint --type-aware ./src",
    "lint:fix": "yarn lint --fix",
    "publish:jsr": "npx -y jsr publish"
  },
  "dependencies": {
    "@storybooker/core": "workspace:^"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.895.0",
    "@aws-sdk/client-s3": "^3.895.0",
    "@types/aws-lambda": "^8.10.152",
    "@types/node": "^22.0.0",
    "@typescript/native-preview": "^7.0.0-dev.20251024.1",
    "oxlint": "^1.24.0",
    "oxlint-tsgolint": "^0.3.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.9"
  },
  "peerDependencies": {
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-dynamodb": {
      "optional": true
    },
    "@aws-sdk/client-s3": {
      "optional": true
    }
  },
  "publishConfig": {
    "exports": {
      "./dynamo-db": "./dist/dynamo-db.js",
      "./lambda": "./dist/lambda.js",
      "./s3": "./dist/s3.js",
      "./package.json": "./package.json"
    }
  }
}
