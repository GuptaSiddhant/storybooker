{
  "name": "@storybooker/adapter-azure",
  "version": "0.5.1",
  "type": "module",
  "description": "StoryBooker Adapter for interacting with Azure services.",
  "author": {
    "name": "Siddhant Gupta",
    "url": "https://guptasiddhant.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guptasiddhant/storybooker",
    "directory": "packages/adapter-azure"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guptasiddhant/storybooker/issues"
  },
  "files": [
    "src",
    "dist",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./blob-storage": {
      "source": "./src/blob-storage.ts",
      "import": "./dist/blob-storage.js",
      "require": "./dist/blob-storage.cjs"
    },
    "./cosmosdb": {
      "source": "./src/cosmosdb.ts",
      "import": "./dist/cosmosdb.js",
      "require": "./dist/cosmosdb.cjs"
    },
    "./data-tables": {
      "source": "./src/data-tables.ts",
      "import": "./dist/data-tables.js",
      "require": "./dist/data-tables.cjs"
    },
    "./easy-auth": {
      "source": "./src/easy-auth.ts",
      "import": "./dist/easy-auth.js",
      "require": "./dist/easy-auth.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "check": "tsgo --noEmit",
    "dev": "tsdown -w ./src",
    "fmt": "prettier src --write --config ../../.prettierrc",
    "lint": "oxlint --type-aware ./src",
    "lint:fix": "yarn lint --fix",
    "start": "func start"
  },
  "dependencies": {
    "@azure/cosmos": "^4.5.1",
    "@azure/data-tables": "^13.0.0",
    "@azure/storage-blob": "^12.0.0",
    "@storybooker/core": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript/native-preview": "^7.0.0-dev.20250823.8",
    "oxlint": "^1.12.0",
    "oxlint-tsgolint": "^0.0.4",
    "prettier": "^3.6.2",
    "tsdown": "^0.14.1"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "./blob-storage": {
        "import": "./dist/blob-storage.js",
        "require": "./dist/blob-storage.cjs"
      },
      "./cosmosdb": {
        "import": "./dist/cosmosdb.js",
        "require": "./dist/cosmosdb.cjs"
      },
      "./data-tables": {
        "import": "./dist/data-tables.js",
        "require": "./dist/data-tables.cjs"
      },
      "./easy-auth": {
        "import": "./dist/easy-auth.js",
        "require": "./dist/easy-auth.cjs"
      },
      "./package.json": "./package.json"
    }
  }
}
